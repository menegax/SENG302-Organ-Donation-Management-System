<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.Label?>
<AnchorPane fx:id="medicationPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0" prefWidth="600.0" stylesheets="@../css/guiStyle.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.GUIDonorMedications">
    <children>
        <Label alignment="CENTER" layoutX="100.0" layoutY="6.0" styleClass="title" text="Medication"/>
        <TextField fx:id="newMedication" focusTraversable="false" layoutX="214.0" layoutY="56.0" prefHeight="25.0" prefWidth="109.0" promptText="Enter a medicine" />
        <Button fx:id="registerMed" layoutX="228.0" layoutY="91.0" mnemonicParsing="false" onAction="#registerMedication"
                prefHeight="25.0" prefWidth="81.0" styleClass="customButton" stylesheets="@../css/guiStyle.css" text="Register" />
        <ListView fx:id="currentMedications" layoutX="214.0" layoutY="130.0" prefHeight="210.0" prefWidth="109.0" />
        <ListView fx:id="medicineInformation" layoutX="337.0" layoutY="14.0" prefHeight="326.0" prefWidth="249.0" />
        <Button fx:id="wipeReview" layoutX="376.0" layoutY="352.0" mnemonicParsing="false" onAction="#refreshReview" prefHeight="25.0"
                prefWidth="80.0" styleClass="customButton" stylesheets="@../css/guiStyle.css" text="Refresh" />
        <Button fx:id="removeMed" layoutX="133.0" layoutY="188.0" mnemonicParsing="false" onAction="#makeHistory" prefHeight="25.0"
                prefWidth="68.0" styleClass="customButton" stylesheets="@../css/guiStyle.css" text="Remove" />
        <Button fx:id="addMed" layoutX="133.0" layoutY="225.0" mnemonicParsing="false" onAction="#makeCurrent" prefHeight="25.0"
                prefWidth="68.0" styleClass="customButton" stylesheets="@../css/guiStyle.css" text="Re-add" />
        <ListView fx:id="pastMedications" layoutX="14.0" layoutY="130.0" prefHeight="210.0" prefWidth="109.0" />
        <Button fx:id="deleteMed" layoutX="27.0" layoutY="352.0" mnemonicParsing="false" onAction="#deleteMedication"
                prefHeight="25.0" prefWidth="81.0" styleClass="customButton" stylesheets="@../css/guiStyle.css" text="Delete" />
        <Button fx:id="clearMed" layoutX="229.0" layoutY="352.0" mnemonicParsing="false" onAction="#clearSelections"
                prefHeight="25.0" prefWidth="81.0" styleClass="customButton" stylesheets="@../css/guiStyle.css" text="Un-select" />
        <Button fx:id="saveMed" layoutX="493.0" layoutY="352.0" mnemonicParsing="false" onAction="#saveMedication"
                prefHeight="25.0" prefWidth="80.0" styleClass="customButton" stylesheets="@../css/guiStyle.css" text="Save" />
        <Button fx:id="goBack" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goToProfile"
                prefHeight="25.0" prefWidth="50.0" styleClass="customButton" stylesheets="@../css/guiStyle.css" text="Back" />
        <Button fx:id="undoEdit" layoutX="14.0" layoutY="52.0" mnemonicParsing="false" onAction="#undo" prefHeight="36.0" prefWidth="31.0" styleClass="undo" stylesheets="@../css/guiStyle.css" />
        <Button fx:id="redoEdit" layoutX="61.0" layoutY="52.0" mnemonicParsing="false" onAction="#redo" prefHeight="36.0" prefWidth="31.0" styleClass="redo" stylesheets="@../css/guiStyle.css" />
    </children>
</AnchorPane>
